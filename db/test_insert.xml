<?xml version="1.0" encoding="UTF-8"?> 
<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml" xml:lang="en-US"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.w3.org/2001/vxml http://www.w3.org/TR/voicexml20/vxml.xsd">

<block>
  <var name="order_num" expr="5"/>
  <var name="item" expr="pizza"/>
  <var name="quantity" expr="1"/>
  <var name="size" expr="large"/>
  <var name="details" expr="['sausage', 'green pepper']"/>
  <var name="price" expr="13.95"/>
  <submit next="http://students.washington.edu/cyc025/db/insert.php" method="post"
    namelist="order_num item quantity size details price" />
</block>
</vxml>